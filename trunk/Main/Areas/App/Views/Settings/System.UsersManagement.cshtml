@inherits WebViewPage<ICollection<SDM.Domain.Models.AccountModel>>
@(Html
.Custom()
.Grid(t => t)
.PropertyColumn("UserName", "Name", t => t.UserName)
.PropertyColumn("IsAdmin", "Admin?", t => t.IsAdmin)
.PropertyColumn("IsEnabled", "Enabled?", t => t.IsEnabled)
.DynamicColumn(null, "Actions", t => Html.Custom().Button("Edit").Action("EditAccount", routeValues: new { id = t.ID }))
.AddButton("Add", "AddAccount"))